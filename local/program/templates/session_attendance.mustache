<div class="rooms">
    <span class="roomsiconimage">{{ iconimage }}</span>
    <h4>{{# str }}session_details, local_program {{/ str }} :</h4>
</div>
<table id = "sessionDetails">
    <tbody>
        <tr>
            <td>
                <div class="col-md-12 p-0">
                    <div class="col-md-6">
                        <p>
                            <span class="label_name">Name <span class="pull-right mr-10">:</span></span>
                            <span>{{ sessiondata.name }}</span>
                        </p>
                        <p>
                            <span class="label_name">Faculty <span class="pull-right mr-10">:</span></span>
                            <span>{{ sessiondata.trainername }}</span>
                        </p>
                    </div>

                    <div class="col-md-6">

                        <p>
                            <span class="label_name">Duration <span class="pull-right mr-10">:</span></span>
                            <span>{{ sessiondata.duration }} Hrs</span>
                        </p>

                        <p>
                            <span class="label_name">Timings <span class="pull-right mr-10">:</span></span>
                            <span>{{#userdate}} {{sessiondata.timestart}}, {{#str}} strftimedatetime {{/str}} {{/userdate}} to {{#userdate}} {{sessiondata.timefinish}}, {{#str}} strftimedatetime {{/str}} {{/userdate}}</span>
                        </p>
                    </div>
                </div>
            </td>
        </tr>
    </tbody>
</table>
