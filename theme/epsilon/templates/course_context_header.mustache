                </div>
            </li>
		{{/ coursebackup}}
		{{# course_complition }}	
                <li>
                	<div class="editingon course_extended_menu_itemcontainer">
                        <a title="{{# str }} coursecompletion {{/ str}}" class="course_extended_menu_itemlink" href="{{config.wwwroot}}/course/completion.php?id={{courseid}}"><i class="icon fa fa-tags fa-fw" aria-hidden="true"></i></a>
                    </div>
                </li>
		{{/ course_complition }}
		{{# course_reports }}
                <li>
                	<div class="editingon course_extended_menu_itemcontainer">
                        <a title="{{# str }} reports {{/ str}}" class="course_extended_menu_itemlink" href="{{ config.wwwroot}}/grade/report/grader/index.php?id={{courseid}}"><i class="icon fa fa-bar-chart" aria-hidden="true"></i></a>
                    </div>
                </li>
		{{/ course_reports }}
		{{# is_courseedit_icon }}
                <li>
                	<div class="courseedit course_extended_menu_itemcontainer">
                        <a title="{{# str }} editcoursesettings {{/ str}}" class="course_extended_menu_itemlink createcoursemodal courseactions" data-action = "createcoursemodal" data-value = {{courseid}} href="javascript:void(0)" onclick="(function(e){ require('local_courses/courseAjaxform').init({contextid:{{categorycontextid}}, component: 'local_courses', callback:'custom_course_form', form_status:0, plugintype:'local', pluginname:'courses', forpurchaseindividually: {{forpurchaseindividually}},courseid: {{ courseid }} }) })(event)"><i class="icon fa fa-pencil" aria-hidden="true"></i></a>
                    </div>
                </li>
		{{/ is_courseedit_icon }}
