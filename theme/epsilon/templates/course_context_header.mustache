            <li>
                <div class="coursebackup course_extended_menu_itemcontainer">
                    <a title="Faculty enrollments" class="course_extended_menu_itemlink" href="{{config.wwwroot}}/local/mooccourses/courseenrol.php?id={{courseid}}&enrolid={{enrolid}}&costcenter={{costcenterid}}&roleid={{roleid}}">
                        <i class="icon fa fa-user-plus fa-fw " aria-hidden="true"></i></a>
                </div>
            </li>
