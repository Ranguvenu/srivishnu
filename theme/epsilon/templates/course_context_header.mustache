				{{/ admin_default_menu }}
			</li>
		{{/ default_menu }}
        {{# programcourse }}
                <li>
                    <div class="editingon course_extended_menu_itemcontainer">
                        <a title="{{# str }} back {{/ str}}" class="course_extended_menu_itemlink" href="{{ linkpath }}"><i class="icon fa fa-arrow-circle-left" aria-hidden="true"></i></a>
                    </div>
                </li>
        {{/ programcourse }}
		{{# userenrollment }}
			<li>
				<div class="coursebackup course_extended_menu_itemcontainer">
                    <a title="User enrollments" class="course_extended_menu_itemlink" href="{{config.wwwroot}}/local/courses/courseenrol.php?id={{courseid}}&enrolid={{enrolid}}"><i class="icon fa fa-user fa-fw " aria-hidden="true"></i></a>
                </div>
