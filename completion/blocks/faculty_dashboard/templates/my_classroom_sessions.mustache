{{#response}}
    {{#viewtypeCard}}
        <table class="generaltable" id="displaysessions">
        
        <thead>
            <tr>
                <th>Name</th>
                <th>Name & Time</th>
                <th>Room</th>
                <th>Status</th>
                <th>Attended Users</th>
                <th>Action</th>
            </tr>
        </thead>
        <tbody>
            {{#records}}
                <tr>
                <td>{{name}}</td>
                <td>{{time}}</td>
                <td>{{room}}</td>
                <td>{{status}}</td>
                <td>{{attendedusers}}</td>
                <td><a href="{{program_url}}">Attendance</a></td>
                </tr>
            {{/records}}
            </tbody>
        </table>
    {{/viewtypeCard}}
{{/response}}



